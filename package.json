{
  "name": "oyvinmar.com",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@glennsl/bs-json": "^3.0.0",
    "body-parser": "^1.18.2",
    "bootstrap-sass": "^3.3.7",
    "bs-fetch": "^0.3.1",
    "compression": "^1.7.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "method-override": "^2.3.10",
    "oauth": "^0.9.15",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "waypoints": "^4.0.1"
  },
  "scripts": {
    "build:static": "parcel build index.html -d dist/static",
    "build:api": "ncc build server/index.js -o dist/api",
    "build": "bsb -make-world && yarn build:static && yarn build:api",
    "clean": "bsb -clean-world",
    "lint-es": "eslint --ignore-path .gitignore *.js server src",
    "lint": "yarn format-check && yarn lint-es",
    "format": "prettier --write \"**/*.{js,jsx,scss,md}\"",
    "format-check": "prettier --list-different \"**/*.{js,jsx,scss,md}\"",
    "prepare": "npm link bs-platform",
    "bsb-watch": "bsb -make-world -w",
    "start": "concurrently -r --kill-others \"yarn bsb-watch\" \"node .\"",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.8",
    "@zeit/ncc": "^0.18.2",
    "await-url": "^0.3.0",
    "chalk": "^2.3.0",
    "concurrently": "^3.5.1",
    "cross-spawn": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "node-sass": "^4.12.0",
    "normalize-url": "^3.0.0",
    "now": "^15.0.11",
    "octonode": "^0.9.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.2.0",
    "reason-react": "^0.7.0",
    "url-regex": "^4.1.1"
  },
  "toolchain": {
    "node": "11.15.0",
    "yarn": "1.15.2"
  }
}
