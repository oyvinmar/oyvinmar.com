  "scripts": {
    "build": "react-app-rewired build --scripts-version reason-scripts",
    "eject": "react-scripts eject",
    "lint":
      "yarn run lint-prettier --silent && eslint --ext .js --ext .jsx ./src",
    "lint-prettier": "node prettier.js lint",
    "pretest": "npm run lint",
    "prepare": "npm link bs-platform",
    "prettier": "node prettier.js write",
    "start":
      "concurrently -r --kill-others \"react-app-rewired start --scripts-version reason-scripts\" \"yarn start-server\"",
    "start-server": "PORT=4000 node server",
    "now-start": "PORT=3000 NODE_ENV=production node server",
    "start-client": "react-app-rewired start --scripts-version reason-scripts",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "bs-jest": "^0.3.1",
    "concurrently": "^3.5.1",
    "normalize-url": "^2.0.1",
    "reason-react": "^0.3.0"
