{
  "name": "oyvinmar.com",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@remix-run/node": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@remix-run/vercel": "^1.7.0",
    "autoprefixer": "^10.2.5",
    "faunadb": "^4.0.0",
    "minipass-fetch": "^1.3.2",
    "oauth": "^0.9.15",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-dom": "^18.2.0",
    "tailwindcss": "^2.2.7"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "NODE_ENV=production remix build",
    "lint-es": "eslint --ignore-path .gitignore",
    "lint": "yarn format-check && yarn lint-es",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,scss,md}\"",
    "format-check": "prettier --list-different \"**/*.{js,jsx,ts,tsx,scss,md}\"",
    "generate:css": "tailwindcss -i ./app/styles/base.css -o ./app/tailwind.css",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.0",
    "@types/oauth": "^0.9.1",
    "@types/react": "^18.0.17",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.0.6",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "vercel": "^28.2.0"
  },
  "volta": {
    "node": "14.15.3",
    "yarn": "1.22.5"
  }
}
