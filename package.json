{
  "name": "oyvinmar.com",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@remix-run/node": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@remix-run/vercel": "^1.7.0",
    "autoprefixer": "^10.4.8",
    "faunadb": "^4.0.0",
    "oauth": "^0.9.15",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "NODE_ENV=production remix build",
    "lint:check": "eslint --ignore-path .gitignore",
    "check-all": "run-s *:check",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,scss,md}\"",
    "format:check": "prettier --list-different \"**/*.{js,jsx,ts,tsx,scss,md}\"",
    "type:check": "yarn tsc",
    "generate:css": "tailwindcss -i ./app/styles/base.css -o ./app/tailwind.css",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.0",
    "@types/oauth": "^0.9.1",
    "@types/react": "^18.0.17",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.0.6",
    "@vercel/node": "^2.5.13",
    "eslint": "^8.20.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.7.4",
    "vercel": "^28.2.0"
  },
  "volta": {
    "node": "16.17.0",
    "pnpm": "7.5.0"
  }
}
