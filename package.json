  "dependencies": {
    "@glennsl/bs-json": "^5.0.2",
    "@oyvinmar-forks/react-collapse": "^5.1.2",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "autoprefixer": "^10.2.5",
    "bs-fetch": "^0.6.2",
    "faunadb": "^4.0.0",
