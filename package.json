{
  "name": "oyvinmar.com",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@glennsl/bs-json": "^3.0.0",
    "bootstrap-sass": "^3.4.1",
    "bs-fetch": "^0.3.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "oauth": "^0.9.15",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "waypoints": "^4.0.1"
  },
  "scripts": {
    "build:bsb": "bsb -make-world",
    "build:static": "parcel build index.html -d dist/static",
    "build:api": "ncc build api/index.js -o dist/api",
    "build": "yarn build:static && yarn build:api",
    "clean": "bsb -clean-world && rm -rf dist",
    "lint-es": "eslint --ignore-path .gitignore *.js api src",
    "lint": "yarn format-check && yarn lint-es",
    "format": "prettier --write \"**/*.{js,jsx,scss,md}\"",
    "format-check": "prettier --list-different \"**/*.{js,jsx,scss,md}\"",
    "bsb-watch": "bsb -make-world -w",
    "start": "node .",
    "test": "yarn build:bsb && jest"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.8",
    "@zeit/ncc": "^0.18.3",
    "await-url": "^0.3.0",
    "bs-platform": "^5.0.4",
    "bsb-js": "^1.1.7",
    "chalk": "^2.4.2",
    "cross-spawn-promise": "^0.10.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "micro": "^9.3.4",
    "node-sass": "^4.12.0",
    "normalize-url": "^4.3.0",
    "now": "^15.2.0",
    "octonode": "^0.9.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "reason-react": "^0.7.0",
    "url-regex": "^5.0.0"
  },
  "toolchain": {
    "node": "11.15.0",
    "yarn": "1.15.2"
  }
}
