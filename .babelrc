{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "IE >= 12"]
        },
        "useBuiltIns": true,
        "modules": false
      }
    ]
  ],
  "plugins": ["transform-object-rest-spread"],
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    },
    // "commonjs": {
    //   "plugins": [["transform-es2015-modules-commonjs"]]
    // },
    "unittest": {
      "plugins": [["transform-es2015-modules-commonjs"]]
    }
  }
}
